<style>
  footer {
    padding: 25px;
    text-align: center;
  }
  .footer_main {
    outline: 0px;
    column-gap: 20px;
    grid-template-columns: 1fr 1fr 1fr 1fr 2fr;
    /* max-width: 1100px; */
    content: "";
  }
</style>
