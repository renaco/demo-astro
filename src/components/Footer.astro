---
const today = new Date();
---

<div class="px-0 pt-2 pb-0 m-0 bg-white text-sky-800" style="outline: 0px;">
  <div
    id="footer_main"
    class="footer_main block clear-both py-5 px-2 m-0 w-full bg-sky-800 text-sky-800"
  >
    <div
      class="float-left p-0 ml-2 w-full text-sky-800"
      style="max-width: 164px; outline: 0px; grid-row: 1 / 4;"
    >
      <span
        class="p-0 my-5 mx-0 font-sans text-lg font-semibold tracking-tight leading-5 text-white uppercase"
        style="outline: 0px;"
      >
        Secciones
      </span>
    </div>
    <div
      class="float-left p-0 w-full text-sky-800"
      style="max-width: 164px; outline: 0px; grid-row: 1 / 4;"
    >
      <div
        class="p-0 my-5 mx-0 font-sans text-lg font-semibold tracking-tight leading-5 text-white uppercase"
        style="outline: 0px;"
      >
        &nbsp;
      </div><span
        class="p-0 my-5 mx-0 font-sans text-lg font-semibold tracking-tight leading-5 text-left text-white uppercase"
        style="outline: 0px; list-style: outside none none;">Suplementos:</span
      >
    </div>
    <div
      class="float-left p-0 w-full text-sky-800"
      id="footer_verticales"
      style="max-width: 164px; outline: 0px; grid-row: 1 / 4;"
    >
    </div>
    <div
      class="float-left p-0 w-full text-sky-800"
      id="footer_locales"
      style="grid-area: 1 / 2 / 2 / 2; outline: 0px; max-width: 164px;"
    >
    </div>
    <div
      class="float-left p-0 w-full max-w-full text-sky-800"
      id="footer_publicidad"
      style="grid-area: 1 / 2 / 2 / 2; outline: 0px;"
    >
    </div>
    <img
      alt="Logo El Universal"
      class="inline mx-0 mt-5 mb-2 w-64 h-8 align-middle cursor-pointer text-sky-800 filter-none"
      src="https://www.eluniversal.com.mx/sites/all/themes/eluniversal/images/logo-el-universal-blanco.png"
      width="350"
      height="38"
      style="display: inline; outline: 0px; max-width: 350px;"
    />
  </div>
</div>
